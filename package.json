  "repository": {
    "type": "git",
    "url": "git+https://github.com/joshbuddy/go-npm.git"
  },
  "homepage": "https://github.com/joshbuddy/go-npm",
  "dependencies": {
    "get-uri": "^6.0.2",
    "mkdirp": "^3.0.1",
    "tar": "^6.2.0",
    "unzipper": "^0.10.10"
  },
  "devDependencies": {
    "esbuild": "^0.20.0",
    "jest": "^29.7.0"
  }
}
